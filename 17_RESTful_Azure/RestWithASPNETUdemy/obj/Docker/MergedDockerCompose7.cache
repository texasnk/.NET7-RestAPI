name: dockercompose12638358073696369457
services:
  db:
    build:
      context: C:\RestWithASP-NET5Udemy\RestWithASP-NET5Udemy\17_RESTful_Azure\RestWithASPNETUdemy
      dockerfile: db/Dockerfile
    command:
    - mysqld
    - --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: rest_with_asp_net_udemy
      MYSQL_PASSWORD: docker
      MYSQL_ROOT_PASSWORD: docker
      MYSQL_USER: docker
      TZ: America/Sao_Paulo
    image: rest-with-asp-net-udemy-db
    networks:
      udemy-network: null
    ports:
    - mode: ingress
      target: 3306
      published: "3308"
      protocol: tcp
    restart: always
  rest-with-asp-net-udemy:
    build:
      context: C:\RestWithASP-NET5Udemy\RestWithASP-NET5Udemy\17_RESTful_Azure\RestWithASPNETUdemy
      dockerfile: RestWithASPNETUdemy/Dockerfile
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      TZ: America/Sao_Paulo
    image: rest-with-asp-net-udemy
    networks:
      udemy-network: null
    ports:
    - mode: ingress
      target: 80
      published: "44300"
      protocol: tcp
networks:
  udemy-network:
    name: dockercompose12638358073696369457_udemy-network
    driver: bridge